<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <record id="view_vehicule_popup_form" model="ir.ui.view">
        <field name="name">vehicule.form</field>
        <field name="model">vehicule</field>
        <field name="arch" type="xml">
            <form string="Vehicule">
                <sheet>
                    <group>
                        <group>
                            <field name="modele" readonly="1" invisible="1"/>
                            <field name="color"/>
                            <field name="zone"/>
                            <field name="matricule"/>
                            <field name="numero"/>
                        </group>
                        <group>
                            <field name="date_debut_service"/>
                            <field name="euro" invisible="1"/>
                            <field name="dinar" invisible="1"/>
                            <field name="prix_achat"/>
                            <field name="prix_achat_eur"/>
                        </group>
                    </group>
                </sheet>
            </form>
        </field>
    </record>

</odoo>